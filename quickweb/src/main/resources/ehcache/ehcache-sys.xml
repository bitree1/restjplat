<?xml version="1.0" encoding="UTF-8"?>
<ehcache updateCheck="false" name="sysCache">

	<diskStore path="java.io.tmpdir/ehcache/xcmquickproject" />

	<!-- DefaultCache setting. -->
	<defaultCache maxElementsInMemory="10000" eternal="false" timeToIdleSeconds="300" timeToLiveSeconds="600"
		overflowToDisk="true" maxElementsOnDisk="100000" />

	<!-- Special objects setting. -->
	<cache name="sysCache" maxElementsInMemory="1000" timeToIdleSeconds="300"  timeToLiveSeconds="600"  
			eternal="true" overflowToDisk="true" maxElementsOnDisk="100000"/>
	
	<cache name="com.restjplat.quickweb.model.Dict" maxElementsInMemory="1000" eternal="true"
		overflowToDisk="true" maxElementsOnDisk="10000" />
			
</ehcache>
